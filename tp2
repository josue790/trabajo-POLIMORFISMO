class Animal:
    def hacerSonido(self):
        pass

class Perro(Animal):
    def hacerSonido(self):
        return "Guau guau"

class Gato(Animal):
    def hacerSonido(self):
        return "Miau miau miau"

def main():
    animales = [Perro(), Gato(), Perro(), Gato()]
    for animal in animales:
        print(animal.hacerSonido())

if __name__ == "__main__":
    main()
