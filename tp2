class Animal:
    def hacer_sonido(self):
        pass 
class Perro(Animal):
    def hacer_sonido(self):
        return "mi perro (nombre: Nala) hace Guauu"
class Gato(Animal):
    def hacer_sonido(self):
        return "mi gato (nombre:macri) hace Miauu"
animales = [Perro(), Gato()]
for animal in animales:
    print(animal.hacer_sonido())
